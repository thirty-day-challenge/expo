{
  "expo": {
    "scheme": "acme",
    "userInterfaceStyle": "automatic",
    "orientation": "portrait",
    "web": {
      "output": "static",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://n"
        }
      ],
      "expo-secure-store",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "name": "30 Day Me",
    "slug": "30-day-me",
    "version": "1.0.0",
    "android": {
      "package": "com.30-day-me.android",
      "permissions": ["android.permission.RECORD_AUDIO"]
    },
    "ios": {
      "bundleIdentifier": "com.your-apple-org-name.your-app-name",
      "buildNumber": "1.0.0"
    },
    "extra": {
      "router": {
        "origin": "https://n"
      },
      "eas": {
        "projectId": "a98f45bb-71e8-4a13-9cd5-42d36e1f5fd0"
      }
    }
  }
}
